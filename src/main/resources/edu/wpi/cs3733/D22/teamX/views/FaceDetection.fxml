<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootElement" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D22.teamX.controllers.FaceDetectionController">
   <children>
      <ImageView fx:id="originalFrame" fitHeight="307.0" fitWidth="313.0" layoutX="144.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="cameraButton" disable="true" layoutX="258.0" layoutY="308.0" mnemonicParsing="false" onAction="#startCamera" text="Start Camera" />
      <CheckBox fx:id="haarClassifier" layoutX="28.0" layoutY="59.0" mnemonicParsing="false" onAction="#haarSelected" text="Haar" />
      <CheckBox fx:id="lbpClassifier" layoutX="29.0" layoutY="85.0" mnemonicParsing="false" onAction="#lbpSelected" text="Lbp" />
   </children>
</AnchorPane>
